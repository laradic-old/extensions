{
    "name": "{vendor}/{package}",
    "type": "laradic-extension",
    "description": "{packageName}",
    "keywords": [
        "{vendor}", "{package}", "{packageName}"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laradic/extensions": "~1.0"
    },
    "require-dev": {
        "laradic/dev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "{autoloadPsr4}": "src/"
        }
    }
}

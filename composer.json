{
    "name": "laradic/extensions",
    "type": "composer-plugin",
    "description": "Laravel package providing extensions",
    "homepage": "https://github.com/laradic/extensions",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "~5.0",
        "laradic/support": "dev-master",
        "laradic/config": "dev-master",
        "composer-plugin-api": "1.0.0"
    },
    "require-dev": {
        "laradic/dev": "dev-master"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Extensions\\": ""
        }
    },
    "extra": {
        "class": [
            "Laradic\\Extensions\\Composer\\ExtensionInstallerPlugin"
        ]
    }
}

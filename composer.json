{
    "name": "laradic/extensions",
    "type": "composer-plugin",
    "description": "Laravel package providing extensions",
    "homepage": "https://github.com/laradic/extensions",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "laradic/support": "~1.2",
        "laradic/console": "~1.0",
        "laradic/config": "~1.3",
        "illuminate/support": "5.0.*",
        "composer-plugin-api": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Extensions\\": "src/"
        }
    },
    "extra": {
        "class": [
            "Laradic\\Extensions\\Composer\\ExtensionInstallerPlugin"
        ]
    }
}
